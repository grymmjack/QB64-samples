  PUT (over, up), smiley, PSET
  CASE CHR$(0) + "H"
  PUT (over, up), nothing, PSET
  IF POINT(over + 10, up - 15) <> 7 THEN up = up - 21
  IF up < 25 THEN up = 408
  PUT (over, up), smiley, PSET
  CASE CHR$(0) + "K"
  PUT (over, up), nothing, PSET
  IF POINT(over - 10, up + 10) <> 7 THEN over = over - 21
  IF over < 9 THEN over = 597
