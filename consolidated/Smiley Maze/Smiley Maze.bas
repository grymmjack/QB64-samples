DECLARE SUB StartGame ()
DIM SHARED Level AS INTEGER
DIM SHARED up AS INTEGER
DIM SHARED over AS INTEGER
DIM SHARED s AS STRING
StartGame
CLS : SCREEN 0: WIDTH 80, 50
Level = 2: length = 80: wide = 46
up = 5: over = 2: s$ = CHR$(178)

MakeLevel:
IF Level = 1 THEN COLOR 7: RESTORE LevelOne
IF Level = 2 THEN COLOR 4: RESTORE LevelTwo
IF Level = 3 THEN COLOR 5: RESTORE LevelThree
IF Level = 4 THEN COLOR 1: RESTORE LevelFour
FOR w = 1 TO wide
    FOR l = 1 TO length
    READ p
    IF p = 1 THEN LOCATE w, l: PRINT CHR$(178)
    IF p = 0 THEN LOCATE w, l: PRINT CHR$(177)
    IF p = 2 THEN LOCATE w, l: PRINT CHR$(0)
    NEXT
NEXT

IF Level = 4 THEN
 row = 5: col = 0: c = 1
 FOR d = 1 TO 12
   c = c + 1
   IF col = 8 THEN row = row + 2
   IF col = 8 THEN col = 0
   col = col + 2
   COLOR c: LOCATE row, col: PRINT CHR$(2)
 NEXT d
END IF
IF Level = 2 THEN up = 5: over = 2
IF Level = 3 THEN up = 5: over = 2
IF Level = 4 THEN up = 45: over = 79
IF Level = 1 THEN COLOR 12: LOCATE up, over: PRINT CHR$(2)
IF Level = 2 THEN COLOR 10: LOCATE up, over: PRINT CHR$(2)
IF Level = 3 THEN COLOR 11: LOCATE up, over: PRINT CHR$(2)
IF Level = 4 THEN COLOR 6: LOCATE up, over: PRINT CHR$(2)

DO
key$ = INKEY$
SELECT CASE key$
  CASE CHR$(0) + "H"
  IF Level = 1 THEN COLOR 7
  IF Level = 2 THEN COLOR 4
  IF Level = 3 THEN COLOR 5
  IF Level = 4 THEN COLOR 1
  LOCATE up, over: PRINT CHR$(177)
  IF NOT SCREEN(up - 1, over) = ASC(s$) THEN up = up - 1
  IF up = 11 AND over = 5 AND Level = 4 THEN GOSUB YouWin
  IF Level = 1 THEN COLOR 12
  IF Level = 2 THEN COLOR 10
  IF Level = 3 THEN COLOR 11
  IF Level = 4 THEN COLOR 6
  LOCATE up, over: PRINT CHR$(2)
  CASE CHR$(0) + "P"
  IF Level = 1 THEN COLOR 7
  IF Level = 2 THEN COLOR 4
  IF Level = 3 THEN COLOR 5
  IF Level = 4 THEN COLOR 1
  LOCATE up, over: PRINT CHR$(177)
  IF NOT SCREEN(up + 1, over) = ASC(s$) THEN up = up + 1
  IF up = 46 AND over = 2 AND Level = 2 THEN Level = 3: GOSUB MakeLevel
  IF Level = 1 THEN COLOR 12
  IF Level = 2 THEN COLOR 10
  IF Level = 3 THEN COLOR 11
  IF Level = 4 THEN COLOR 6
  LOCATE up, over: PRINT CHR$(2)
  CASE CHR$(0) + "M"
  IF Level = 1 THEN COLOR 7
  IF Level = 2 THEN COLOR 4
  IF Level = 3 THEN COLOR 5
  IF Level = 4 THEN COLOR 1
  LOCATE up, over: PRINT CHR$(177)
  IF NOT SCREEN(up, over + 1) = ASC(s$) THEN over = over + 1
  IF up = 5 AND over = 80 AND Level = 1 THEN Level = 2: GOSUB MakeLevel
  IF Level = 1 THEN COLOR 12
  IF Level = 2 THEN COLOR 10
  IF Level = 3 THEN COLOR 11
  IF Level = 4 THEN COLOR 6
  LOCATE up, over: PRINT CHR$(2)
  CASE CHR$(0) + "K"
  IF Level = 1 THEN COLOR 7
  IF Level = 2 THEN COLOR 4
  IF Level = 3 THEN COLOR 5
  IF Level = 4 THEN COLOR 1
  LOCATE up, over: PRINT CHR$(177)
  IF NOT SCREEN(up, over - 1) = ASC(s$) THEN over = over - 1
  IF up = 45 AND over = 1 AND Level = 3 THEN Level = 4: GOSUB MakeLevel
  IF Level = 1 THEN COLOR 12
  IF Level = 2 THEN COLOR 10
  IF Level = 3 THEN COLOR 11
  IF Level = 4 THEN COLOR 6
  LOCATE up, over: PRINT CHR$(2)
  CASE ELSE
END SELECT
LOOP UNTIL key$ = CHR$(27)
END

YouWin:
COLOR 15
LOCATE 19, 21: PRINT "                                        "
LOCATE 20, 21: PRINT "   Congrats, you found your long lost   "
LOCATE 21, 21: PRINT "    friends!! Your life is now happy.   "
LOCATE 22, 21: PRINT "                                        "
DO
LOOP UNTIL INKEY$ <> ""
SYSTEM

